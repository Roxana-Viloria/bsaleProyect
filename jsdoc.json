{
    "source" : {
        "include" : ["src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs)"
    },
    "opts": {
        "recurse":"true",
        "destination": "./docs"
    },
    "templates":{
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "plugins": ["plugins/markdown"]
}